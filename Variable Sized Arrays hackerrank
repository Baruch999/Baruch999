
int main() {
    /* Enter your code here. Read input from STDIN. Print output to STDOUT */       
   int n, q; 
   cin>>n>>q;  
   vector<int>arr[n] ;  
   for(int i=0;i<n;i++) 
   { 
       int k ; 
       cin>>k ; 
    for(int j=0;j<k;j++)
      { int m ; 
        cin>>m;

        arr[i].push_back(m);  
      }
   } 
   int x,y ;
   for(int i=0;i<q;i++)  
    {
     cin>>x>>y ;
     cout<<arr[x][y]<<endl;
    } 
    








    return 0;
}
